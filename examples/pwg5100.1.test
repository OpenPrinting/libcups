{
    NAME "PWG 5100.1-2022: Finishings Attributes"
    OPERATION Get-Printer-Attributes

    GROUP operation-attributes-tag
    ATTR charset attributes-charset utf-8
    ATTR language attributes-natural-language en
    ATTR uri printer-uri $uri

    STATUS successful-ok

    # Check for all REQUIRED Printer Description attributes
    EXPECT finishing-template-supported IF-DEFINED REQUIRE_FIN
    EXPECT finishings-col-database IF-DEFINED REQUIRE_FIN
    EXPECT finishings-col-default IF-DEFINED REQUIRE_FIN
    EXPECT finishings-col-ready IF-DEFINED REQUIRE_FIN
    EXPECT finishings-default IF-DEFINED REQUIRE_FIN
    EXPECT finishings-ready IF-DEFINED REQUIRE_FIN
    EXPECT finishings-supported OF-TYPE enum WITH-VALUE >3 IF-DEFINED REQUIRE_FIN
    EXPECT printer-finisher IF-DEFINED REQUIRE_FIN
    EXPECT printer-finisher-description IF-DEFINED REQUIRE_FIN

    # Determine whether finishings are supported
    EXPECT finishings-supported OF-TYPE enum WITH-VALUE >3 DEFINE-MATCH PRINTER_HAS_FINISHINGS
    EXPECT finishings-supported WITH-VALUE 3 IF-DEFINED PRINTER_HAS_FINISHINGS

    EXPECT finishings-supported WITH-VALUE 4 DEFINE-MATCH PRINTER_HAS_FINISHINGS_STITCHING       # staple
    EXPECT finishings-supported WITH-VALUE 5 DEFINE-MATCH PRINTER_HAS_FINISHINGS_PUNCHING        # punch
    EXPECT finishings-supported WITH-VALUE 6 DEFINE-MATCH PRINTER_HAS_FINISHINGS_COVERING        # cover
    EXPECT finishings-supported WITH-VALUE 7 DEFINE-MATCH PRINTER_HAS_FINISHINGS_BINDING         # bind
    EXPECT finishings-supported WITH-VALUE 8 DEFINE-MATCH PRINTER_HAS_FINISHINGS_STITCHING       # saddle-stitch
    EXPECT finishings-supported WITH-VALUE 9 DEFINE-MATCH PRINTER_HAS_FINISHINGS_STITCHING       # edge-stitch
    EXPECT finishings-supported WITH-VALUE 10 DEFINE-MATCH PRINTER_HAS_FINISHINGS_FOLDING        # fold
    EXPECT finishings-supported WITH-VALUE 11 DEFINE-MATCH PRINTER_HAS_FINISHINGS_TRIMMING       # trim
    EXPECT finishings-supported WITH-VALUE 12 DEFINE-MATCH PRINTER_HAS_FINISHINGS_BALING         # bale
    EXPECT finishings-supported WITH-VALUE 13 DEFINE-MATCH PRINTER_HAS_FINISHINGS_BOOKLET_MAKER  # booklet-maker

    EXPECT finishings-supported WITH-VALUE 15 DEFINE-MATCH PRINTER_HAS_FINISHINGS_COATING        # coat
    EXPECT finishings-supported WITH-VALUE 16 DEFINE-MATCH PRINTER_HAS_FINISHINGS_LAMINATING     # laminate

    EXPECT finishings-supported WITH-VALUE 20 DEFINE-MATCH PRINTER_HAS_FINISHINGS_STITCHING      # staple-top-left
    EXPECT finishings-supported WITH-VALUE 21 DEFINE-MATCH PRINTER_HAS_FINISHINGS_STITCHING      # staple-bottom-left
    EXPECT finishings-supported WITH-VALUE 22 DEFINE-MATCH PRINTER_HAS_FINISHINGS_STITCHING      # staple-top-right
    EXPECT finishings-supported WITH-VALUE 23 DEFINE-MATCH PRINTER_HAS_FINISHINGS_STITCHING      # staple-bottom-right
    EXPECT finishings-supported WITH-VALUE 24 DEFINE-MATCH PRINTER_HAS_FINISHINGS_STITCHING      # edge-stitch-left
    EXPECT finishings-supported WITH-VALUE 25 DEFINE-MATCH PRINTER_HAS_FINISHINGS_STITCHING      # edge-stitch-top
    EXPECT finishings-supported WITH-VALUE 26 DEFINE-MATCH PRINTER_HAS_FINISHINGS_STITCHING      # edge-stitch-right
    EXPECT finishings-supported WITH-VALUE 27 DEFINE-MATCH PRINTER_HAS_FINISHINGS_STITCHING      # edge-stitch-bottom
    EXPECT finishings-supported WITH-VALUE 28 DEFINE-MATCH PRINTER_HAS_FINISHINGS_STITCHING      # staple-dual-left
    EXPECT finishings-supported WITH-VALUE 29 DEFINE-MATCH PRINTER_HAS_FINISHINGS_STITCHING      # staple-dual-top
    EXPECT finishings-supported WITH-VALUE 30 DEFINE-MATCH PRINTER_HAS_FINISHINGS_STITCHING      # staple-dual-right
    EXPECT finishings-supported WITH-VALUE 31 DEFINE-MATCH PRINTER_HAS_FINISHINGS_STITCHING      # staple-dual-bottom
    EXPECT finishings-supported WITH-VALUE 32 DEFINE-MATCH PRINTER_HAS_FINISHINGS_STITCHING      # staple-triple-left
    EXPECT finishings-supported WITH-VALUE 33 DEFINE-MATCH PRINTER_HAS_FINISHINGS_STITCHING      # staple-triple-top
    EXPECT finishings-supported WITH-VALUE 34 DEFINE-MATCH PRINTER_HAS_FINISHINGS_STITCHING      # staple-triple-right
    EXPECT finishings-supported WITH-VALUE 35 DEFINE-MATCH PRINTER_HAS_FINISHINGS_STITCHING      # staple-triple-bottom

    EXPECT finishings-supported WITH-VALUE 50 DEFINE-MATCH PRINTER_HAS_FINISHINGS_BINDING        # bind-left
    EXPECT finishings-supported WITH-VALUE 51 DEFINE-MATCH PRINTER_HAS_FINISHINGS_BINDING        # bind-top
    EXPECT finishings-supported WITH-VALUE 52 DEFINE-MATCH PRINTER_HAS_FINISHINGS_BINDING        # bind-right
    EXPECT finishings-supported WITH-VALUE 53 DEFINE-MATCH PRINTER_HAS_FINISHINGS_BINDING        # bind-bottom

    EXPECT finishings-supported WITH-VALUE 60 DEFINE-MATCH PRINTER_HAS_FINISHINGS_TRIMMING       # trim-after-pages
    EXPECT finishings-supported WITH-VALUE 61 DEFINE-MATCH PRINTER_HAS_FINISHINGS_TRIMMING       # trim-after-documents
    EXPECT finishings-supported WITH-VALUE 62 DEFINE-MATCH PRINTER_HAS_FINISHINGS_TRIMMING       # trim-after-copies
    EXPECT finishings-supported WITH-VALUE 63 DEFINE-MATCH PRINTER_HAS_FINISHINGS_TRIMMING       # trim-after-job

    EXPECT finishings-supported WITH-VALUE 70 DEFINE-MATCH PRINTER_HAS_FINISHINGS_PUNCHING       # punch-top-left
    EXPECT finishings-supported WITH-VALUE 71 DEFINE-MATCH PRINTER_HAS_FINISHINGS_PUNCHING       # punch-bottom-left
    EXPECT finishings-supported WITH-VALUE 72 DEFINE-MATCH PRINTER_HAS_FINISHINGS_PUNCHING       # punch-top-right
    EXPECT finishings-supported WITH-VALUE 73 DEFINE-MATCH PRINTER_HAS_FINISHINGS_PUNCHING       # punch-bottom-right
    EXPECT finishings-supported WITH-VALUE 74 DEFINE-MATCH PRINTER_HAS_FINISHINGS_PUNCHING       # punch-dual-left
    EXPECT finishings-supported WITH-VALUE 75 DEFINE-MATCH PRINTER_HAS_FINISHINGS_PUNCHING       # punch-dual-top
    EXPECT finishings-supported WITH-VALUE 76 DEFINE-MATCH PRINTER_HAS_FINISHINGS_PUNCHING       # punch-dual-right
    EXPECT finishings-supported WITH-VALUE 77 DEFINE-MATCH PRINTER_HAS_FINISHINGS_PUNCHING       # punch-dual-bottom
    EXPECT finishings-supported WITH-VALUE 78 DEFINE-MATCH PRINTER_HAS_FINISHINGS_PUNCHING       # punch-triple-left
    EXPECT finishings-supported WITH-VALUE 79 DEFINE-MATCH PRINTER_HAS_FINISHINGS_PUNCHING       # punch-triple-top
    EXPECT finishings-supported WITH-VALUE 80 DEFINE-MATCH PRINTER_HAS_FINISHINGS_PUNCHING       # punch-triple-right
    EXPECT finishings-supported WITH-VALUE 81 DEFINE-MATCH PRINTER_HAS_FINISHINGS_PUNCHING       # punch-triple-bottom
    EXPECT finishings-supported WITH-VALUE 82 DEFINE-MATCH PRINTER_HAS_FINISHINGS_PUNCHING       # punch-quad-left
    EXPECT finishings-supported WITH-VALUE 83 DEFINE-MATCH PRINTER_HAS_FINISHINGS_PUNCHING       # punch-quad-top
    EXPECT finishings-supported WITH-VALUE 84 DEFINE-MATCH PRINTER_HAS_FINISHINGS_PUNCHING       # punch-quad-right
    EXPECT finishings-supported WITH-VALUE 85 DEFINE-MATCH PRINTER_HAS_FINISHINGS_PUNCHING       # punch-quad-bottom
    EXPECT finishings-supported WITH-VALUE 86 DEFINE-MATCH PRINTER_HAS_FINISHINGS_PUNCHING       # punch-multiple-left
    EXPECT finishings-supported WITH-VALUE 87 DEFINE-MATCH PRINTER_HAS_FINISHINGS_PUNCHING       # punch-multiple-top
    EXPECT finishings-supported WITH-VALUE 88 DEFINE-MATCH PRINTER_HAS_FINISHINGS_PUNCHING       # punch-multiple-right
    EXPECT finishings-supported WITH-VALUE 89 DEFINE-MATCH PRINTER_HAS_FINISHINGS_PUNCHING       # punch-multiple-bottom

    EXPECT finishings-supported WITH-VALUE 90 DEFINE-MATCH PRINTER_HAS_FINISHINGS_FOLDING        # fold-accordion
    EXPECT finishings-supported WITH-VALUE 91 DEFINE-MATCH PRINTER_HAS_FINISHINGS_FOLDING        # fold-double-gate
    EXPECT finishings-supported WITH-VALUE 92 DEFINE-MATCH PRINTER_HAS_FINISHINGS_FOLDING        # fold-gate
    EXPECT finishings-supported WITH-VALUE 93 DEFINE-MATCH PRINTER_HAS_FINISHINGS_FOLDING        # fold-half
    EXPECT finishings-supported WITH-VALUE 94 DEFINE-MATCH PRINTER_HAS_FINISHINGS_FOLDING        # fold-half-z
    EXPECT finishings-supported WITH-VALUE 95 DEFINE-MATCH PRINTER_HAS_FINISHINGS_FOLDING        # fold-left-gate
    EXPECT finishings-supported WITH-VALUE 96 DEFINE-MATCH PRINTER_HAS_FINISHINGS_FOLDING        # fold-letter
    EXPECT finishings-supported WITH-VALUE 97 DEFINE-MATCH PRINTER_HAS_FINISHINGS_FOLDING        # fold-parallel
    EXPECT finishings-supported WITH-VALUE 98 DEFINE-MATCH PRINTER_HAS_FINISHINGS_FOLDING        # fold-poster
    EXPECT finishings-supported WITH-VALUE 99 DEFINE-MATCH PRINTER_HAS_FINISHINGS_FOLDING        # fold-right-gate
    EXPECT finishings-supported WITH-VALUE 100 DEFINE-MATCH PRINTER_HAS_FINISHINGS_FOLDING       # fold-z
    EXPECT finishings-supported WITH-VALUE 101 DEFINE-MATCH PRINTER_HAS_FINISHINGS_FOLDING       # fold-engineering-z

    # Check valid syntax...
    EXPECT finishing-template-supported OF-TYPE keyword|name IN-GROUP printer-attributes-tag IF-DEFINED PRINTER_HAS_FINISHINGS
    EXPECT finishings-col-database OF-TYPE collection IN-GROUP printer-attributes-tag IF-DEFINED PRINTER_HAS_FINISHINGS
    EXPECT finishings-col-default OF-TYPE collection|no-value IN-GROUP printer-attributes-tag IF-DEFINED PRINTER_HAS_FINISHINGS
    EXPECT finishings-col-ready OF-TYPE collection|no-value IN-GROUP printer-attributes-tag IF-DEFINED PRINTER_HAS_FINISHINGS
    EXPECT finishings-col-supported OF-TYPE keyword IN-GROUP printer-attributes-tag WITH-VALUE finishing-template IF-DEFINED PRINTER_HAS_FINISHINGS
    EXPECT finishings-default OF-TYPE enum IN-GROUP printer-attributes-tag IF-DEFINED PRINTER_HAS_FINISHINGS
    EXPECT finishings-ready OF-TYPE enum IN-GROUP printer-attributes-tag WITH-ALL-VALUES-FROM finishings-supported IF-DEFINED PRINTER_HAS_FINISHINGS
    EXPECT finishings-supported OF-TYPE enum IN-GROUP printer-attributes-tag WITH-VALUE 3 IF-DEFINED PRINTER_HAS_FINISHINGS

    EXPECT finishings-col-supported WITH-VALUE baling IF-DEFINED PRINTER_HAS_FINISHINGS_BALING
    EXPECT baling-type-supported OF-TYPE keyword|name IN-GROUP printer-attributes-tag IF-DEFINED PRINTER_HAS_FINISHINGS_BALING
    EXPECT baling-when-supported OF-TYPE keyword IN-GROUP printer-attributes-tag IF-DEFINED PRINTER_HAS_FINISHINGS_BALING

    EXPECT finishings-col-supported WITH-VALUE binding IF-DEFINED PRINTER_HAS_FINISHINGS_BINDING
    EXPECT binding-reference-edge-supported OF-TYPE keyword IN-GROUP printer-attributes-tag IF-DEFINED PRINTER_HAS_FINISHINGS_BINDNG
    EXPECT binding-type-supported OF-TYPE keyword|name IN-GROUP printer-attributes-tag IF-DEFINED PRINTER_HAS_FINISHINGS_BINDING

    EXPECT finishings-col-supported WITH-VALUE coating IF-DEFINED PRINTER_HAS_FINISHINGS_COATING
    EXPECT coating-sides-supported OF-TYPE keyword IN-GROUP printer-attributes-tag IF-DEFINED PRINTER_HAS_FINISHINGS_COATNG
    EXPECT coating-type-supported OF-TYPE keyword|name IN-GROUP printer-attributes-tag IF-DEFINED PRINTER_HAS_FINISHINGS_COATING

    EXPECT finishings-col-supported WITH-VALUE covering IF-DEFINED PRINTER_HAS_FINISHINGS_COVERING
    EXPECT covering-name-supported OF-TYPE keyword|name IN-GROUP printer-attributes-tag IF-DEFINED PRINTER_HAS_FINISHINGS_COVERING

    EXPECT finishings-col-supported WITH-VALUE folding IF-DEFINED PRINTER_HAS_FINISHINGS_FOLDING
    EXPECT folding-direction-supported OF-TYPE keyword IN-GROUP printer-attributes-tag IF-DEFINED PRINTER_HAS_FINISHINGS_FOLDING
    EXPECT folding-offset-supported OF-TYPE integer|rangeOfInteger IN-GROUP printer-attributes-tag IF-DEFINED PRINTER_HAS_FINISHINGS_FOLDNG
    EXPECT folding-reference-edge-supported OF-TYPE keyword IN-GROUP printer-attributes-tag IF-DEFINED PRINTER_HAS_FINISHINGS_FOLDING

    EXPECT finishings-col-supported WITH-VALUE laminating IF-DEFINED PRINTER_HAS_FINISHINGS_LAMINATING
    EXPECT laminating-sides-supported OF-TYPE keyword IN-GROUP printer-attributes-tag IF-DEFINED PRINTER_HAS_FINISHINGS_LAMINATING
    EXPECT laminating-type-supported OF-TYPE keyword|name IN-GROUP printer-attributes-tag IF-DEFINED PRINTER_HAS_FINISHINGS_LAMINATING

    EXPECT finishings-col-supported WITH-VALUE punching IF-DEFINED PRINTER_HAS_FINISHINGS_PUNCHING
    EXPECT punching-locations-supported OF-TYPE integer|rangeOfInteger IN-GROUP printer-attributes-tag IF-DEFINED PRINTER_HAS_FINISHINGS_PUNCHING
    EXPECT punching-offset-supported OF-TYPE integer|rangeOfInteger IN-GROUP printer-attributes-tag IF-DEFINED PRINTER_HAS_FINISHINGS_PUNCHING
    EXPECT punching-reference-edge-supported OF-TYPE keyword IN-GROUP printer-attributes-tag IF-DEFINED PRINTER_HAS_FINISHINGS_PUNCHING

    EXPECT finishings-col-supported WITH-VALUE stitching IF-DEFINED PRINTER_HAS_FINISHINGS_STITCHING
    EXPECT stitching-angle-supported OF-TYPE integer|rangeOfInteger IN-GROUP printer-attributes-tag IF-DEFINED PRINTER_HAS_FINISHINGS_STITCHING
    EXPECT stitching-locations-supported OF-TYPE integer|rangeOfInteger IN-GROUP printer-attributes-tag IF-DEFINED PRINTER_HAS_FINISHINGS_STITCHING
    EXPECT stitching-method-supported OF-TYPE keyword IN-GROUP printer-attributes-tag IF-DEFINED PRINTER_HAS_FINISHINGS_STITCHING
    EXPECT stitching-offset-supported OF-TYPE integer|rangeOfInteger IN-GROUP printer-attributes-tag IF-DEFINED PRINTER_HAS_FINISHINGS_STITCHING
    EXPECT stitching-reference-edge-supported OF-TYPE keyword IN-GROUP printer-attributes-tag IF-DEFINED PRINTER_HAS_FINISHINGS_STITCHING

    EXPECT finishings-col-supported WITH-VALUE trimming IF-DEFINED PRINTER_HAS_FINISHINGS_TRIMMING
    EXPECT trimming-offset-supported OF-TYPE integer|rangeOfInteger IN-GROUP printer-attributes-tag IF-DEFINED PRINTER_HAS_FINISHINGS_TRIMMING
    EXPECT trimming-reference-edge-supported OF-TYPE keyword IN-GROUP printer-attributes-tag IF-DEFINED PRINTER_HAS_FINISHINGS_TRIMMING
    EXPECT trimming-type-supported OF-TYPE keyword|name IN-GROUP printer-attributes-tag IF-DEFINED PRINTER_HAS_FINISHINGS_TRIMMING
    EXPECT trimming-when-supported OF-TYPE keyword IN-GROUP printer-attributes-tag IF-DEFINED PRINTER_HAS_FINISHINGS_TRIMMING

    EXPECT ?job-pages-per-set-supported OF-TYPE boolean IN-GROUP printer-attributes-tag COUNT 1

    EXPECT printer-finisher OF-TYPE octetString IN-GROUP printer-attributes-tag SAME-COUNT-AS printer-finisher-description IF-DEFINED PRINTER_HAS_FINISHINGS
    EXPECT printer-finisher-description OF-TYPE text IN-GROUP printer-attributes-tag SAME-COUNT-AS printer-finisher IF-DEFINED PRINTER_HAS_FINISHINGS

    EXPECT ?printer-finisher-supplies OF-TYPE octetString IN-GROUP printer-attributes-tag SAME-COUNT-AS printer-finisher-supplies-description IF-DEFINED PRINTER_HAS_FINISHINGS
    EXPECT ?printer-finisher-supplies-description OF-TYPE text IN-GROUP printer-attributes-tag SAME-COUNT-AS printer-finisher-supplies IF-DEFINED PRINTER_HAS_FINISHINGS
}


# Baling
{
    SKIP-IF-DEFINED NOPRINT
    SKIP-IF-NOT-DEFINED PRINTER_HAS_FINISHINGS_BALING

    NAME "Print-Job with finishings 'bale'"
    OPERATION Print-Job

    GROUP operation-attributes-tag
    ATTR charset attributes-charset utf-8
    ATTR language attributes-natural-language en
    ATTR uri printer-uri $uri
    ATTR name requesting-user-name $user
    ATTR mimeMediaType document-format $filetype

    GROUP job-attributes-tag
    ATTR integer copies 1
    ATTR enum finishings 12 # bale

    FILE $filename

    STATUS successful-ok

    EXPECT job-id OF-TYPE integer IN-GROUP job-attributes-tag WITH-VALUE >0 COUNT 1
    EXPECT job-uri OF-TYPE uri IN-GROUP job-attributes-tag COUNT 1
}
{
    SKIP-IF-DEFINED NOPRINT
    SKIP-IF-NOT-DEFINED PRINTER_HAS_FINISHINGS_BALING

    NAME "Verify finishings 'bale'"
    OPERATION Get-Job-Attributes

    GROUP operation-attributes-tag
    ATTR charset attributes-charset utf-8
    ATTR language attributes-natural-language en
    ATTR uri printer-uri $uri
    ATTR integer job-id $job-id
    ATTR name requesting-user-name $user

    STATUS successful-ok

    EXPECT job-uri OF-TYPE uri IN-GROUP job-attributes-tag COUNT 1 WITH-VALUE $job-uri
    EXPECT job-state OF-TYPE enum IN-GROUP job-attributes-tag COUNT 1

    EXPECT finishings OF-TYPE enum IN-GROUP job-attributes-tag WITH-VALUE 12 COUNT 1
}


